{
    "name": "@fangzhongya/vue-archive",
    "private": false,
    "version": "0.0.2-13",
    "type": "module",
    "description ": "vue 组件注释生成文档",
    "author": "fangzhongya ",
    "license": "MIT",
    "keywords": [
        "vue 组件注释生成文档"
    ],
    "repository": {
        "type": "git",
        "url": " https://github.com/fangzhongya/vue-archive.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "dev": "vite",
        "dev:vue": "ts-node node.vue.ts",
        "dev:v": "ts-node node.version.ts",
        "dev:tests": "ts-node node.tests.ts",
        "build": "vue-tsc && vite build",
        "pub": "pnpm publish --access public --no-git-checks",
        "all": "pnpm build & pnpm dev:v & pnpm pub",
        "build:tsup": "tsup",
        "preview": "vite preview",
        "test": "vitest test"
    },
    "devDependencies": {
        "@fangzhongya/create": "0.2.2-41",
        "@fangzhongya/utils": "0.0.7-14",
        "@fangzhongya/vue-components": "0.1.3-2",
        "@highlightjs/vue-plugin": "^2.1.0",
        "@types/node": "^18.14.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "comment-parser": "^1.3.1",
        "es-module-lexer": "^1.1.1",
        "highlight.js": "^11.7.0",
        "markdown-it": "^13.0.1",
        "prettier": "^2.8.4",
        "sass": "^1.58.3",
        "tsup": "^6.6.3",
        "typescript": "^4.9.5",
        "unplugin-vue-components": "^0.24.0",
        "vite": "^4.1.2",
        "vite-plugin-dts": "2.0.0-beta.1",
        "vitest": "^0.28.5",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6",
        "vue-tsc": "^1.1.3"
    },
    "main": "./dist/lib/packages/index.js",
    "module": "./dist/es/packages/index.js",
    "files": [
        "dist"
    ]
}